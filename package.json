{
  "name": "eviso-boilerplate",
  "version": "2.0",
  "description": "eVISO Backend Boilerplate",
  "author": "Tiago Campos <tiago.campos@eviso.it>",
  "maintainers": [],
  "private": true,
  "repository": "",
  "scripts": {
    "start": "cross-env NODE_ENV=development ts-node -r tsconfig-paths/register src/main.ts -i max",
    "prestart:dev": "yarn lint",
    "start:dev": "nodemon",
    "start:dev-windows":"cross-env NODE_ENV=development nest start --watch",
    "start:debug": "nodemon --config nodemon-debug.json",
    "prestart:prod": "rimraf dist && tsc && yarn lint",
    "start:prod": "cross-env NODE_ENV=production node dist/main.js",
    "tscheck:w": "tsc -w",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.ts?(x)\" --max-warnings 0",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "hmr": "node dist/server",
    "webpack": "webpack --config webpack.config.js",
    "deploy:local": "docker-compose --env-file .env.development up -d"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "yarn format",
      "yarn lint",
      "git add ."
    ]
  },
  "dependencies": {
    "@bugsnag/plugin-express": "^7.9.0",
    "@nestjs-modules/mailer": "^1.5.1",
    "@nestjs/common": "^6.10.14",
    "@nestjs/core": "^6.10.14",
    "@nestjs/jwt": "^6.1.1",
    "@nestjs/mapped-types": "*",
    "@nestjs/passport": "^7.1.5",
    "@nestjs/schedule": "^0.4.3",
    "@nestjs/typeorm": "^6.2.0",
    "@nkaurelien/nest-bugsnag": "^6.1.0",
    "@types/cron": "^1.7.2",
    "@types/ua-parser-js": "^0.7.35",
    "cache-manager": "^3.4.4",
    "crypto": "^1.0.1",
    "express": "^4.17.1",
    "handlebars": "^4.7.7",
    "helmet": "^3.21.2",
    "jwks-rsa": "^1.12.3",
    "jwt-decode": "^3.1.2",
    "moment": "^2.29.1",
    "mongodb": "^3.4.1",
    "mysql": "^2.18.1",
    "nestjs-i18n": "~6.0.0",
    "newrelic": "^7.3.1",
    "nodemailer": "^6.5.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "path": "^0.12.7",
    "pg": "^7.17.1",
    "reflect-metadata": "^0.1.13",
    "rss-parser": "^3.12.0",
    "rxjs": "^6.5.4",
    "swagger-ui-express": "^4.1.6",
    "typeorm": "^0.2.22",
    "ua-parser-js": "^0.7.26"
  },
  "devDependencies": {
    "@nestjs/swagger": "^4.1.10",
    "@nestjs/testing": "^6.10.14",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.2",
    "@types/helmet": "^0.0.45",
    "@types/jest": "^24.0.25",
    "@types/joi": "^14.3.4",
    "@types/node": "^13.1.6",
    "@types/nodemailer": "^6.4.1",
    "@types/passport-jwt": "^3.0.3",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^4.29.0",
    "@typescript-eslint/parser": "^4.29.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.11.0",
    "cross-env": "^6.0.3",
    "dotenv": "^8.2.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^4.0.7",
    "jest": "^24.9.0",
    "joi": "^14.3.1",
    "lint-staged": "^9.5.0",
    "nodemon": "^2.0.2",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.0",
    "supertest": "^4.0.2",
    "ts-jest": "^24.3.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.1",
    "tsconfig-paths": "^3.9.0",
    "tslint": "5.20.1",
    "typescript": "^4.2.4",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-node-externals": "^1.7.2"
  }
}
